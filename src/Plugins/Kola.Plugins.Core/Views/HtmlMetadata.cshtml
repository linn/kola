@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<Kola.Domain.Instances.AtomInstance>
@using Kola.Domain.Extensions
@{
    var name = Model.Properties.GetAsAttribute("name");
    var content = Model.Properties.GetAsAttribute("content");
    var httpEquiv = Model.Properties.GetAsAttribute("httpEquiv");
}
    <meta @Html.Raw(httpEquiv)@Html.Raw(name)@Html.Raw(content)/>